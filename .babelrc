{
  "presets" : ["@babel/preset-env","@babel/preset-react"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": [
          "./"
        ],
        "alias": {
          "common": "./common",
          "controller": "./controller",
          "static": "./static",
          "assets": "./assets",
          "pages": "./pages"
        }
      }
    ],
    [
      "@babel/plugin-transform-arrow-functions"
    ],
    "@babel/plugin-syntax-dynamic-import"
  ],
  "env": {
    "production": {
      "plugins": [
        "transform-remove-console"
      ]
    }
  }
}
